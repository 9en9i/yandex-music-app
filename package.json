{
  "name": "yandex-music-app",
  "version": "1.2.0",
  "author": "juvirez",
  "description": "just webview for yandex music with native features",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/juvirez/yandex-music-app.git"
  },
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "publish": "build -p always"
  },
  "devDependencies": {
    "electron": "^2.0.4",
    "electron-builder": "^20.24.4"
  },
  "dependencies": {
    "electron-ga": "^1.0.6",
    "electron-media-service": "^0.2.2",
    "electron-settings": "^3.2.0",
    "electron-updater": "^3.0.3"
  },
  "build": {
    "productName": "Yandex Music (Unofficial)",
    "appId": "com.github.juvirez.yandexmusicapp",
    "mac": {
      "category": "public.app-category.music",
      "target": [
        "zip",
        "dmg"
      ],
      "icon": "src/static/icon.icns"
    }
  }
}
